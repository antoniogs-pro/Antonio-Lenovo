Host_Alias	VPN = 192.168.0.0/255.255.0.0, 10.0.0.0/255.0.0.0
User_Alias	OPERATORS =  sysop
Runas_Alias     OPERATOR = root, apache, sysop, antonio
Cmnd_Alias	SYSOP = /usr/bin/git, /usr/bin/vi, /bin/nano, /bin/cat, /bin/acturepo, /bin/sleep

%ITI ALL=(OPERATOR:ALL) NOPASSWD: SYSOP
OPERATORS  VPN=(OPERATOR:ALL) NOEXEC: SYSOP
